package paquete;

import java.awt.List;
import java.io.File;
import java.io.FileNotFoundException;
import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Scanner;
import java.util.TreeSet;

public class Main {
	
	private ArrayList<Pais> paises = new ArrayList<Pais>();
	
	public Main(String entrada) throws FileNotFoundException {
		Scanner in = new Scanner(new File(entrada));
		
		int cantidad = in.nextInt();
		in.nextLine();
		
		for (int i = 0; i < cantidad; i++) {
			Pais pais = new Pais(in.next());
			Pais limitrofe = new Pais(in.next());
			
			if(paises.contains(pais)) {
				System.out.println("Ya lo tengo " + pais);
				paises.get(paises.indexOf(pais)).agregarLimitrofe(limitrofe.getNombre());
			}
			else {
				System.out.println("No lo tengo " + pais);
				pais.agregarLimitrofe(limitrofe.getNombre());
				paises.add(pais);
			}
			
			if( paises.contains(limitrofe) ) {
				System.out.println("Ya lo tengo " + limitrofe);
				paises.get(paises.indexOf(limitrofe)).agregarLimitrofe(pais.getNombre());
			}
			else {
				System.out.println("No lo tengo " + limitrofe);
				limitrofe.agregarLimitrofe(pais.getNombre());
				paises.add(limitrofe);
			}
				
		}
		
		for (Pais pais : paises) {
			System.out.println(pais);
		}
		
		in.close();
	}

	public static void main(String[] args) throws FileNotFoundException {
		Main m = new Main("paises.in");

	}

}
