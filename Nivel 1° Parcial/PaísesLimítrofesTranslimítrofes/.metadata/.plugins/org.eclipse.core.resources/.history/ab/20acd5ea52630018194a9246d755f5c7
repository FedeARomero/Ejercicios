package paquete;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.ArrayList;
import java.util.Map;
import java.util.Scanner;
import java.util.TreeMap;

public class Limitrofes {
	
	private Map<Pais, Integer> paises;
	
	public Limitrofes(String entrada) throws FileNotFoundException {
		Scanner in = new Scanner(new File(entrada));
		paises = new TreeMap<Pais, Integer>(new ComparePais());
		int cantidad = in.nextInt();
		in.nextLine();
		for (int i = 0; i < cantidad; i++) {
			Pais pais;
			Pais limitrofe;
			
			this.paises.put(pais = new Pais(in.next()), this.paises.containsKey(pais) ? this.paises.get(pais) + 1 : 1);
			this.paises.put(limitrofe = new Pais(in.next()), this.paises.containsKey(limitrofe) ? this.paises.get(limitrofe) + 1 : 1);
			
			pais.agregarLimitrofe(limitrofe.getNombre());
			limitrofe.agregarLimitrofe(pais.getNombre());
			
			System.out.println(pais);
			System.out.println(limitrofe);
		}
		in.close();
	}

	public static void main(String[] args) {
		
		Limitrofes l;
		try {
			l = new Limitrofes("paises.in");
//			l.mostrar();
		} catch (FileNotFoundException evento) {
			System.out.println(evento.getMessage());
			evento.printStackTrace();
		}
	}

}
